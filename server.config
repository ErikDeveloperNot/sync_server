# network configs
#bind_address=localhost
bind_address=ec2-13-56-39-109.us-west-1.compute.amazonaws.com
bind_port=8443
#service threads specifies starting thread count, can grow to max connections cout, will shrink also
service_threads=10
ssl=true
server_cert=/opt/registration_server/cert.pem
server_key=/opt/registration_server/privkey.pem
server_max_connections=250
server_keep_alive=15
server_use_keep_alive_cleaner=false
server_tcp_back_log=100

# controls how many pending requests can be for the same account before 500 errors are returned
max_account_pending_ops=3

# db settings
db_server=192.168.56.102
db_port=5432
db_password=passvault-secret
db_username=passvault-user
db_name=passvault
db_cleaner=true
db_cleaner_interval=1
db_cleaner_purge_days=30
